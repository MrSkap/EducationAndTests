{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": {
      "Console": {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss.fff} [{Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    }
  }
}